{
  "name": "@media-center/app-tv",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android --no-packager --appIdSuffix .debug",
    "android:clean": "cd android && ./gradlew clean ; cd -",
    "android:release": "react-native run-android --mode Release --no-packager",
    "android:bundle": "react-native bundle --platform android --dev true --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res && cd android && ./gradlew assembleDebug ; cd -",
    "ios": "react-native run-ios --no-packager",
    "tvos": "expo run:ios --scheme timotex-tvOS --device \"Apple TV\"",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "start": "react-native start",
    "test": "jest",
    "update": "eas update",
    "typecheck": "tsc --build",
    "typecheck:open": "code `yarn typecheck | grep error | cut -d \"(\" -f 1 | uniq | xargs`"
  },
  "dependencies": {
    "@gorhom/portal": "^1.0.14",
    "@media-center/algorithm": "*",
    "@media-center/server": "*",
    "@media-center/turbo-vlc": "*",
    "@preact/signals-core": "^1.5.0",
    "@react-native-async-storage/async-storage": "^1.21.0",
    "@react-native-community/blur": "^4.3.2",
    "@react-native-masked-view/masked-view": "0.3.0",
    "axios": "^1.5.0",
    "eas-cli": "^5.9.1",
    "expo": "^50.0.3",
    "expo-updates": "~0.24.8",
    "lottie-react-native": "^6.5.1",
    "pretty-format": "^26.5.2",
    "react": "18.2.0",
    "react-native": "npm:react-native-tvos@0.73.2-0",
    "react-native-bootsplash": "^5.2.2",
    "react-native-linear-gradient": "^2.8.2",
    "react-native-reanimated": "^3.6.1",
    "react-native-sse": "^1.2.0",
    "react-native-svg": "14.1.0",
    "react-native-vector-icons": "^10.0.0",
    "react-router-native": "^6.15.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/plugin-proposal-decorators": "^7.23.6",
    "@babel/plugin-transform-flow-strip-types": "^7.23.3",
    "@babel/plugin-transform-private-methods": "^7.23.3",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@media-center/dev": "*",
    "@react-native/babel-preset": "0.73.19",
    "@react-native/eslint-config": "0.73.2",
    "@react-native/metro-config": "0.73.3",
    "@react-native/typescript-config": "0.73.1",
    "@types/react": "^18.2.6",
    "@types/react-native-vector-icons": "^6.4.14",
    "babel-jest": "^29.6.3",
    "metro-react-native-babel-preset": "0.76.8"
  },
  "engines": {
    "node": ">=20"
  }
}
